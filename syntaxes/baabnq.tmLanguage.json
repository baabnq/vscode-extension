{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "baabnq",
	"patterns": [
		{ 
			"include": "#comments"
		},
		{ 
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#eos"
		}
	],
	"repository": {
		"comments": {
			"begin": "\"",
			"end": "\n",
			"name": "comment.baabnq"
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.control.baabnq",
				"match": "\\b(jump|lab)\\b"
			}, {
				"name": "keyword.other.baabnq",
				"match": "\\b(put|print|input|use)\\b"
			}, {
				"name": "entity.name.function",
				"match": "\\b(pull|push|sub|return)\\b"
			}, {
				"name": "entity.name.class",
				"match": "\\b(new|static|->|<-)\\b"
			}]
		},
		"variables": {
			"match": "\\b_\\w+\\b",
			"name": "variable.baabnq"
		},
		"strings": {
			"name": "string.quoted.double.baabnq",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.baabnq",
					"match": "\\\\."
				}
			]
		},
		"eos": {
			"name": "constant.language",
			"match": ";"
		}
	},
	"scopeName": "source.baabnq"
}