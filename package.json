{
  "name": "baabnq",
  "displayName": "Baabnq",
  "description": "Baabnq language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "languages": [{
      "id": "baabnq",
      "aliases": ["baabnq", "baabnq"],
      "extensions": [".baabnq"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "baabnq",
      "scopeName": "source.baabnq",
      "path": "./syntaxes/baabnq.tmLanguage.json"
    }],
    "commands": [{
      "command": "baabnq.run",
      "title": "Run Baabnq"
    }],
    "keybindings":  [{
      "command": "baabnq.run",
      "key": "F5",
      "when": "editorLangId == baabnq"
    }]

  }
}
